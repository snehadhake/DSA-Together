class Solution{
    public:
    // Function to search x in arr
    
    // X: element to be searched for
    int search(int arr[100], int N, int X)
    {
        for(int i=0;i<N;i++)
        {
            if(arr[i]==X)
            {
                return i;
            }
        }
        return -1;
    }

};
